<?xml version="1.0"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SpawnGroups>
    <!-- SMALL GRIDS -->

    <!-- Single Tank Easy Convoy -->
    <SpawnGroup>
      <Id>
        <TypeId>SpawnGroupDefinition</TypeId> <!--Do Not Edit-->
        <SubtypeId>Earth-Rockslinger-SG</SubtypeId>
      </Id>
      <Description>
      [Modular Encounters SpawnGroup]
        
      [AtmosphericCargoShip:true]
      [LunarCargoShip:true]
      [RivalAiSpawn:true]
      [RivalAiAtmosphericSpawn:true]

      [BlockReplacerProfileNames:MES-DisposableNpcBeacons]
      [BlockReplacerProfileNames:MES-ProprietaryCompRichBlocks]
      [BlockReplacerProfileNames:MES-ProprietaryValuableBlocks]
      [CargoShipTerrainPath:true]
      [ClearAuthorship:true]
      [CustomPathStartAltitude:0]
      [CustomPathEndAltitude:0]
      [FactionOwner:EARTH]
      [IgnoreGlobalBlockReplacer:true]
      [MaxSpawnFromPlanetSurface:2000]
      [ModulesForArmorReplacement:MyObjectBuilder_RadioAntenna/MES-Suppressor-Jetpack-Large]
      [ReplaceArmorBlocksWithModules:true]
      [ReplenishSystems:true]
      [RivalAiReplaceRemoteControl:true]
      [SkipAirDensityCheck:true]
      [ThreatScoreMinimum:25]
      [ThreatScoreMaximum:250]      
      [UseBlockReplacerProfile:true]
      [UseRivalAi:true]
      [UseThreatLevelCheck:true]
      </Description>
      <IsPirate>true</IsPirate> <!--Do not edit-->
      <Frequency>4.0</Frequency>
      <Prefabs>
        <Prefab SubtypeId="Earth-Rockslinger">
          <BeaconText>Rockslinger</BeaconText>
          <Behaviour>RAI-Behavior-Earth-Cargo</Behaviour>
          <Position>
            <X>0.0</X>
            <Y>0.0</Y>
            <Z>0.0</Z>
          </Position>
          <Speed>15</Speed>					
        </Prefab>
      </Prefabs>
    </SpawnGroup>

