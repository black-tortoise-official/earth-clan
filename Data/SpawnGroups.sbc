    <!-- LARGE GRIDS -->
    
    <!-- Convoy Site - Single Convoy Vehicle -->
    <SpawnGroup>
      <Id>
        <TypeId>SpawnGroupDefinition</TypeId> <!--Do Not Edit-->
        <SubtypeId>Earth-ConvoySite-Single-SG</SubtypeId>
      </Id>
      <Description>
      [Modular Encounters SpawnGroup]
        
      [PlanetaryInstallation:true]
      [PlanetaryInstallationType:Small]
      [RivalAiSpawn:true]
      [RivalAiSpaceSpawn:false]

      [BlockReplacerProfileNames:MES-DisposableNpcBeacons]
      [BlockReplacerProfileNames:MES-ProprietaryCompRichBlocks]
      [BlockReplacerProfileNames:MES-ProprietaryValuableBlocks]
      [ClearAuthorship:true]
      [FactionOwner:EARTH]
      [IgnoreGlobalBlockReplacer:true]
      [MaxSpawnFromPlanetSurface:2000]
      [ModulesForArmorReplacement:MyObjectBuilder_RadioAntenna/MES-Suppressor-Jetpack-Large]
      [ModulesForArmorReplacement:MyObjectBuilder_RadioAntenna/MES-Suppressor-Drill-Large]
      [RandomGridNamePattern:(Convoy-Relay) GoodAdjective JobNoun GreekLetter]
      [ReplaceAntennaNameWithRandomizedName:Antenna]
      [ReplaceArmorBlocksWithModules:true]
      [ReplenishSystems:true]
      [RivalAiReplaceRemoteControl:true]
      [SkipAirDensityCheck:true]
      [ThreatScoreMinimum:25]
      [ThreatScoreMaximum:250]
      [UseBlockReplacerProfile:true]
      [UseRandomNameGenerator:true]
      [UseRivalAi:true]
      [UseThreatLevelCheck:true]
      </Description>
      <IsPirate>true</IsPirate> <!--Do not edit-->
      <Frequency>2.0</Frequency>
      <Prefabs>
        <Prefab SubtypeId="Earth-ConvoySite-Alpha">
          <BeaconText>Convoy Relay Site Small</BeaconText>
          <Behaviour>RAI-Behavior-Earth-Convoysite-Single</Behaviour>
          <Position>
            <X>0.0</X>
            <Y>-4.0</Y>
            <Z>0.0</Z>
          </Position>
          <Speed>0</Speed>					
        </Prefab>
      </Prefabs>
    </SpawnGroup>

